import java.util.*;
class beginner101
{
  public static void main(String ar[])
  {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    if(a>0)
    {
    System.out.print("positive");
    }
    else if(a<0)
    {
    System.out.print("Negative"):
    }
    else
    {
    System.out.print("zero"):
    }
  }
}
